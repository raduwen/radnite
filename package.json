{
    "name": "radnite",
    "version": "0.0.1",
    "description": "",
    "main": "index.js",
    "scripts": {
        "preview-dev": "parcel server src/index.html       --public-url /        --out-dir public --cache-dir .cache/preview",
        "admin-dev": "parcel server src/admin/index.html --public-url /admin/  --out-dir public/admin --cache-dir .cache/admin",
        "preview-build": "parcel build src/index.html       --public-url /       --out-dir public",
        "admin-build": "parcel build src/admin/index.html --public-url /admin/ --out-dir public/admin",
        "build": "yarn preview-build && yarn admin-build"
    },
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "eslint": "^5.11.0",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-node": "^8.0.0",
        "eslint-plugin-promise": "^4.0.1",
        "eslint-plugin-standard": "^4.0.0",
        "parcel-bundler": "^1.11.0",
        "prettier": "^1.15.3",
        "sass": "^1.15.2",
        "standard": "^12.0.1"
    },
    "dependencies": {
        "express": "^4.16.4",
        "prop-types": "^15.6.2",
        "react": "^16.7.0",
        "react-dom": "^16.7.0",
        "sanitize.css": "^8.0.0"
    }
